{
  "name": "package-json",
  "displayName": "",
  "description": "这是一个快捷执行package.json命令的 vs code 扩展, 搭配Vue,React 体验更佳 ~",
  "version": "1.0.0",
  "publisher": "package-json",
  "keywords": [
    "btc",
    "vscode"
  ],
  "icon": "img/icon.png",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:package.json"
  ],
  "main": "./extension",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "package",
          "title": "package-json",
          "icon": "img/icon.svg"
        }
      ]
    },
    "views": {
      "package": [
        {
          "id": "packageScript",
          "name": "项目脚本"
        },
        {
          "id": "extensionScript",
          "name": "扩展脚本"
        }
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "eslint": "^4.11.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "fs-extra": "^8.1.0",
    "open": "^6.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chenzejiang/vscode-package-json"
  },
  "__npminstall_done": false
}
